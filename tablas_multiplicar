
#tablas pares
print("tablas de multiplicar pares")
for i in range(1, 11):
    if i % 2 == 0:
        for j in range(1, 11):
            print(i, 'x', j, '=', i*j)
        print() 
        
# tablas impares
print("tablas de multiplicar impares")
for i in range(1, 11):
    if i % 2 != 0:
        for j in range(1, 11):
            print(i, 'x', j, '=', i*j)
        print()